Private Sub get_format_info()
    Dim SelectedSheet As Worksheet
    Dim C_Width() As Double
    Dim R_Height(1 To 202) As Double
    Dim str As String
    Dim i As Integer
    
    Open "d:\downloads\format.txt" For Output As #1
    
    Set SelectedSheet = Worksheets("CDA_1")
    For i = 1 To 23
        ReDim Preserve C_Width(1 To i)
        'C_Width(i) = SelectedSheet.Columns(i).ColumnWidth
        str = str + CStr(SelectedSheet.Columns(i).ColumnWidth) + ","
    Next
    Write #1, str;
    Print #1, vbNewLine & "****" & vbCrLf;
    str = ""
    For i = 1 To 202
        'R_Height(i) = SelectedSheet.Rows(i).RowHeight
        str = str + CStr(SelectedSheet.Rows(i).RowHeight) + ","
    Next
    Write #1, str
    Close #1
End Sub
Sub get_merge_color_info()
    Dim SelectedSheet As Worksheet
    Dim rng As Range
    Dim rngStart As Range
    Dim rngEnd As Range
    
    Set SelectedSheet = Worksheets("CDA_1")
    Set rng = Range("B2")

    If rng.MergeCells Then

        Set rng = rng.MergeArea
        Set rngStart = rng.Cells(1, 1)
        Set rngEnd = rng.Cells(rng.Rows.Count, rng.Columns.Count)

        MsgBox "First Cell " & rngStart.Address & vbNewLine & "Last Cell " & rngEnd.Address

    Else

        MsgBox "Not merged area"

    End If

End Sub
Private Sub get_text()
    Dim i As Integer
    Dim j As Integer
    Dim str As String
    Dim SelectedSheet As Worksheet
    Set SelectedSheet = Worksheets("CDA_1")
    Open "d:\downloads\texts.txt" For Output As #2
    With SelectedSheet
        For i = 1 To 202
            For j = 1 To 23
                If IsEmpty(Cells(i, j).Value2) Then
                Else
                    str = CStr(Cells(i, j).Value2)
                    str = Replace(str, """", """""")
                    str = Replace(str, vbLf, """ & Chr(10) & """)
                    str = ".Cells(" + CStr(i) + "," + CStr(j) + ").Value2 = " + """" + str + """"
                    Print #2, str
                    str = ".Cells(" + CStr(i) + "," + CStr(j) + ").VerticalAlignment = " + CStr(Cells(i, j).VerticalAlignment)
                    Print #2, str
                    str = ".Cells(" + CStr(i) + "," + CStr(j) + ").HorizontalAlignment = " + CStr(Cells(i, j).HorizontalAlignment)
                    Print #2, str
                    str = ".Cells(" + CStr(i) + "," + CStr(j) + ").Font.Size = " + CStr(Cells(i, j).Font.Size)
                    Print #2, str
                    If Not IsNull(.Cells(i, j).Font.Color) Then
                        str = ".Cells(" + CStr(i) + "," + CStr(j) + ").Font.Color = " + CStr(Cells(i, j).Font.Color)
                        Print #2, str
                    End If
                    str = ".Cells(" + CStr(i) + "," + CStr(j) + ").Font.Bold = " + CStr(Cells(i, j).Font.Bold)
                    Print #2, str
                    str = ".Cells(" + CStr(i) + "," + CStr(j) + ").Font.Italic = " + CStr(Cells(i, j).Font.Italic)
                    Print #2, str
                End If
            Next
        Next
    End With
    Close #2
End Sub
